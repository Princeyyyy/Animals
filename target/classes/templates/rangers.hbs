{{#partial "title"}}Wildlife Tracker{{/partial}}
{{#partial "content"}}

    <div class="rangers">
        {{#if rangers}}

            <h3>Rangers:</h3>
            <table class="table table-dark">
                <thead>
                <tr>
                    <th scope="col">Name</th>

                    <th scope="col">Badge</th>

                    <th scope="col">Details</th>
                </tr>
                </thead>
                <tbody>
                {{#each rangers}}
                    <tr>
                        <td>{{name}}</td>

                        <td>{{badgeNo}}</td>


                        <td><a href="/rangers/{{id}}" class="btn btn-primary">Details</a></td>


                    </tr>
                {{/each}}
                </tbody>
            </table>
        {{/if}}

    </div>

    {{/partial}}
{{> base.hbs}}